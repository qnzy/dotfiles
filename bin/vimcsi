SESSIONNAME="VIMCSI"
cd ~/code/scheme
tmux has-session -t $SESSIONNAME > /dev/null
if [ $? != 0 ]
    then
        echo "creating new session"
        tmux new-session -s $SESSIONNAME -d
        tmux send-keys 'vim' 'C-m'
        tmux split-window
        tmux send-keys 'csi' 'C-m'
        tmux select-pane -U
fi

tmux attach -t $SESSIONNAME
